{
  "name": "es6-playground",
  "version": "0.0.1",
  "description": "playing wiht es6",
  "main": "app.js",
  "scripts": {
    "env": "evn",
    "build-js": "browserify -t [babelify --presets [ es2015 react ] --sourceMaps=true] app/src/App.es6 > dist/app.js",
    "build": "npm run build-js",
    "prestart": "npm run build",
    "start": "http-server ./dist -a localhost -p 1337"
  },
  "author": "Caleb Eno",
  "license": "MIT",
  "dependencies": {
    "react": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^2.12.0",
    "http-server": "^0.9.0"
  }
}
